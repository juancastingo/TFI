@model BIZ.BIZProvincia

@{
    ViewBag.Title = "Create";
}

<h2>Create</h2>

@using (Html.BeginForm())
{
    @Html.ValidationSummary(true)

    <fieldset>
        <legend>BIZProvincia</legend>
        <table class="table table-bordered table-striped" cellspacing="0" width="100%">
            <tr>
                <td>@Html.LabelFor(model => model.Nombre)</td>
                <td>
                        @Html.EditorFor(model => model.Nombre)
                        @Html.ValidationMessageFor(model => model.Nombre)
                </td>
            </tr>
            <tr>
                <td>
                    @Html.LabelFor(model => model.IDPais, "Pais")
                </td>
                <td>
                    @Html.DropDownList("IDPais")
                    @Html.ValidationMessageFor(model => model.IDPais)
                </td>
            </tr>
        </table>
            <input type="submit" class="btn btn-primary" value="Create" />
            @Html.ActionLink("Back to List", "Index", new { id = "algo" }, new { @class = "btn btn-success", @style = "color:white" })

    </fieldset>
}



@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
